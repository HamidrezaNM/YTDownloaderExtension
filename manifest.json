{
  "manifest_version": 3,
  "name": "YT Downloader",
  "version": "1.0",
  "description": "This is a description of my extension",
  "permissions": [
    "tabs",
    "webNavigation"
  ],
  "icons": {
    "48": "icons/icon48.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}