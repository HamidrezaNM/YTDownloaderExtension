{
  "manifest_version": 2,
  "name": "YT Downloader",
  "version": "1.1",
  "description": "This is a description of my extension",
  "permissions": [
    "tabs",
    "webNavigation",
    "*://ytdownloader-api-chi.vercel.app/*"
  ],
  "icons": {
    "48": "icons/icon48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}
